    "version": "0.2.0",
    "configurations": [
        {
            "name": "ROS: Launch",
            "type": "ros",
            "request": "launch",
            "target": "${workspaceFolder}/src/robocrop/perception/oak_camera/launch/oak_camera.launch"
        },
        {
            "name": "(lldb) Launch",
            "type": "lldb",
            "request": "launch",
            "program": "${workspaceFolder}/devel/lib/arm_control/TestActuators",
            "cwd": "${workspaceFolder}"
        },
        {
            "name": "(lldb) Attach",
            "type": "lldb",
            "request": "attach",
            "pid": "${command:pickProcess}"
        }
    ]
}